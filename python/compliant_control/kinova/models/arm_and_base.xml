<mujoco model="arm_and_base">
    <compiler angle="radian" meshdir="meshes/" autolimits="true"/>
    <asset>
        <texture type="skybox" builtin="gradient" rgb1=".3 .5 .7" rgb2="0 0 0" width="512" height="512"/>
        <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
        <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2"/>
    </asset>
    <worldbody>
        <geom name="floor" size="0 0 .05" type="plane" material="grid"/>
        <light pos="0 0 5" dir="0 0 -1" diffuse="1 1 1"/>
        <include file="clearpath/clearpath_dingo_o.xml"/>
        <include file="kinova/kinova_gen3_lite.xml"/>
    </worldbody>
    <equality>
        <weld body1="base" body2="BASE"/>
    </equality>
    <include file="clearpath/attributes.xml"/>
    <include file="kinova/attributes.xml"/>
    <include file="mecanum/attributes.xml"/>
</mujoco>